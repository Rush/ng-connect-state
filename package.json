{
  "name": "connect-state-playground",
  "version": "0.0.0",
  "description": "Helper function for angular to allow creation of reactive components",
  "license": "MIT",
  "scripts": {
    "ngcc": "ngcc",
    "postinstall": "ngcc",
    "ng": "ng",
    "lint": "ng lint",
    "test": "ng test --project connect-state --runInBand",
    "test:watch": "ng test --project connect-state --runInBand --watch",
    "test:integration": "ng test --project integration --runInBand",
    "test:integration:watch": "ng test --project integration --runInBand --watch",
    "build": "ng-packagr -p src/package.json && cpx README.md dist",
    "build:integration": "ng build --project integration --prod",
    "serve:integration": "ng serve --project integration --port 8889",
    "serve:integration:static": "angular-http-server --path dist-integration -p 4200 -o"
  },
  "maintainers": [
    "Damian Kaczmarek <rush@rushbase.net>"
  ],
  "keywords": [
    "Angular easy observables",
    "Angular reactive components",
    "Angular connect state",
    "Angular easy state",
    "Angular component observables"
  ],
  "devDependencies": {
    "@angular-builders/jest": "~13.0.0",
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular/cli": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/jest": "^25.2.3",
    "@types/node": "^16.0.0",
    "angular-http-server": "^1.9.0",
    "cpx": "^1.5.0",
    "glob": "^7.1.6",
    "jest": "^27.0.0",
    "ng-packagr": "~13.3.0",
    "prettier": "^1.19.1",
    "rxjs": "^6.5.2",
    "ts-morph": "^7.1.2",
    "typescript": "^4.6.0",
    "zone.js": "0.11.4",
    "@ngneat/until-destroy": "^9.0.0"
  },
  "dependencies": {
    "minimist": "^1.2.5"
  }
}
